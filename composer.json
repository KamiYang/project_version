{
  "name": "kamiyang/ext-projectversion",
  "type": "typo3-cms-extension",
  "description": "Displays current project version based on 'VERSION' file or GIT revision.",
  "homepage": "https://github.com/KamiYang/project_version",
  "version": "0.1.1",
  "time": "2018-05-24",
  "license": "GPL-3.0+",
  "keywords": [
    "TYPO3",
    "CMS",
    "typo3",
    "GIT",
    "git",
    "version"
  ],
  "authors": [
    {
      "name": "Jan Stockfisch",
      "email": "jan.stockfisch@googlemail.com",
      "homepage": "http://www.jan-stockfisch.de/"
    }
  ],
  "replace": {
    "project_version": "self.version"
  },
  "require": {
    "typo3/cms": ">=8.7.0 <=8.7.15",
    "php": ">=7.0"
  },
  "require-dev": {
    "nimut/testing-framework": "~3.0.7",
    "phpunit/phpunit": "~6.5"
  },
  "autoload": {
    "psr-4": {
      "KamiYang\\ProjectVersion\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KamiYang\\ProjectVersion\\Tests\\": "Tests/"
    }
  }
}
